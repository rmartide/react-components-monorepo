      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/react-typescript@3.1.53"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  },
  "devDependencies": {
    "@testing-library/react": "^9.5.0"
  }
